###############################################################################
#                                Gpass Config                                 #
#                                                                             #
###############################################################################
#
#
#Lenght of password
password_len = 25
#
#Character Sets {char_set = Title:setOfCharacters}
char_set = alpha:abcdefghijklmnopqurstuvwxyz
char_set = ALPHA:ABCDEFGHIJKLMNOPQURSTUVWXYZ
char_set = int:0123456789
char_set = symbols:!@#$%^&*()_-+={}|[]?><~`
#Small symbol set
char_set = ssymbols:!@#$&*?
#
#default characters to use
char_sets_to_use = alpha, ALPHA, int, ssymbols
#
#GPG binary
gpgbinary = /usr/bin/gpg2
#
#GPG home
gpghome = $HOME/.gnupg
#
#Default GPG key
gpgkey = cmo.uwp.2010@gmail.com
#
#Pass Password Store home
password_store = $HOME/.password-store
